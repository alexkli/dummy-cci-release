{
  "plugins": [
    ["@semantic-release/commit-analyzer", {
      "preset": "eslint",
      "releaseRules": [
        {
          "tag": "Breaking",
          "release": "major"
        },
        {
          "tag": "breaking",
          "release": "major"
        },
        {
          "tag": "New",
          "release": "minor"
        },
        {
          "tag": "new",
          "release": "minor"
        },
        {
          "subject": "no-release:",
          "release": false
        },
        {
          "subject": "!no-release:*",
          "release": "patch"
        }
      ],
      "parserOpts": {
        "noteKeywords": [
          "BREAKING CHANGE",
          "BREAKING CHANGES"
        ]
      }
    }],
    ["@semantic-release/release-notes-generator", {
      "preset": "eslint",
      "presetConfig": {
        "message": ".*",
        "subject": ".*"
      }
    }],
    ["@semantic-release/npm"],
    ["@semantic-release/github"]
  ]
}